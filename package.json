{
  "name": "mb-cm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.13",
    "npm": "please-use-yarn",
    "yarn": ">= 1.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:preuat": "tsc && vite build --mode preuat",
    "build:sit": "tsc && vite build --mode sit",
    "lint": "eslint .",
    "lint:fix": "eslint \"./src/**/*.{js,jsx,ts,tsx,json}\" --fix",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,scss,md,html,json}\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "postinstall": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.44.0",
    "@tanstack/react-query-devtools": "^5.44.0",
    "antd": "^5.23.1",
    "axios": "1.8.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "jotai": "^2.11.0",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.23.1",
    "recharts": "^2.15.1",
    "sass": "^1.77.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.10.7",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "^5.7.3",
    "vite": "^5.2.0",
    "vite-plugin-svgr": "^4.2.0"
  }
}
